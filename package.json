		"@types/webxr": "^0.5.14",
		"@types/w3c-image-capture": "^1.0.10",
		"babel-preset-minify": "^0.5.2",
		"prettier": "^2.3.2",
		"ts-loader": "^9.2.3",
		"tslint": "^6.1.3",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^4.3.5",
		"webpack": "^5.44.0",
